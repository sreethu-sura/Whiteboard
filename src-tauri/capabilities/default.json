{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit"
  ],
  "scope": {
    "event": {
      "allow": ["tauri://file-drop", "tauri://drop", "tauri://drag-drop", "*"]
    }
  }
}